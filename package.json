{
  "name": "python-spa-client-webpack-starter-kit",
  "version": "1.0.0",
  "description": "a starter kit for using python to serve an API or SPA",
  "main": "index.js",
  "scripts": {
    "build": "webpack -p --progress --colors --profile",
    "postinstall": "virtualenv venv --system-site-packages",
    "prestart": "webpack --progress --profile",
    "start": "cd dist && python main.py"
  },
  "author": "Erik Yuzwa <hello@erikyuzwa.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "html-webpack-plugin": "^2.29.0",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1"
  },
  "dependencies": {
    "jquery": "^3.2.1"
  }
}
